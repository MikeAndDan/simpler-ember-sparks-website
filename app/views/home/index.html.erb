<section>
  <div class="jumbotron text-center">


    <div class="container">
      <div class="row">
          <h1>The largest library of ember screencasts</h1>
          <br>
          <h2 class="modified-headline">
            <!-- Random testimonial -->
          </h2>
          <a class="btn btn-ghost" href="/#video_archive">View Library</a>
      </div>
    </div>
  </div>

</section>
<section>
  <div class="container">
    <h3>Featured Classes</h3>
    <div class="row">

      <% Tag.limit(3).where(tag_type: 'series').order('most_recent_publish_date DESC').each do |series| %>
        <div class="col-md-4">
          <div class="well">
            <li class="media">
              <div class="media-body">
                <%= link_to series do %><h4 class="media-heading"><%= series.name %></h4><% end %>
                Length: <%= series.taggings.length %> videos, <%= series.seconds / 60 %> minutes
                <br/>
                Most recent video: <%= series.most_recent_publish_date %>
              </div>
            </li>
          </div>
        </div>
      <% end %>

    </div>
  </div>
</section>

